# PostgreSQL Database Connection
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=weather_db
POSTGRES_USER=airflow_weather_user
POSTGRES_PASSWORD=your_secure_password_here

# dbt Configuration
DBT_PROFILES_DIR=.
DBT_PROJECT_DIR=.

# Airflow Configuration (if running dbt via Airflow)
AIRFLOW_HOME=/path/to/airflow
AIRFLOW__CORE__DAGS_FOLDER=/path/to/dags

# Email Notifications
NOTIFICATION_EMAIL=your_email@company.com

# Optional: dbt Cloud (if using dbt Cloud instead of local)
# DBT_CLOUD_ACCOUNT_ID=your_account_id
# DBT_CLOUD_PROJECT_ID=your_project_id
# DBT_CLOUD_API_TOKEN=your_api_token
